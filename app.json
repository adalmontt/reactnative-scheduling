{
  "expo": {
    "scheme": "scheduling",
    "name": "reactnative-scheduling",
    "slug": "reactnative-scheduling",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appIcon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.adalmont.reactnativescheduling"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-audio"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "226bf4cc-3b55-41d1-b4c5-af6e681b13d5"
      }
    }
  }
}
